echo Restarting WebViews server for $1.
cd ~/svn/Oblomov/WebViews
killall $1
dist/build/$1/$1 nodebug > serverOutput$1.txt 2>&1 &

# Wait a bit and show initial output to see if the server actually started
# (cannot use $? because background shell command always exits succesfully)
sleep 0.1
cat serverOutput$1.txt


# when put in crontab, use:
#    @reboot ~/svn/Oblomov/WebViews/restartServer <Executable>  >/dev/null 2>&1
